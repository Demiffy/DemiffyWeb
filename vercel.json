{
  "rewrites": [
    {
      "source": "/user-active",
      "destination": "http://109.80.40.13:7000/user-active"
    },
    {
      "source": "/getgrades/(.*)",
      "destination": "https://109.80.40.13:443/getgrades"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
